<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ra1vi2.asdc.kietian">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/asdc"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Student_main"
            android:label="@string/title_activity_student_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".Profile_change_stdnt"
            android:label="@string/title_activity_profile_change_stdnt"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SendMessage"
            android:label="@string/title_activity_send_message"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ComposeMessage"
            android:label="@string/title_activity_compose_message"
            android:theme="@style/AppTheme" />
        <activity android:name=".ShowInboxMsg" />
        <activity
            android:name=".Faculty_main"
            android:label="@string/title_activity_faculty_main"
            android:theme="@style/AppTheme" />
        <activity android:name=".FindTeacher"></activity>
    </application>

</manifest>
